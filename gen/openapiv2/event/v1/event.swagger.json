    }
  },
  "tags": [
    {
      "name": "EventService"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/events/record": {
      "post": {
        "summary": "记录直播事件",
        "description": "接收并存储各种类型的直播事件，如聊天消息、礼物、用户状态等。",
        "operationId": "EventService_RecordEvent",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1RecordEventResponse"
            }
          },
          "404": {
            "description": "Resource not found.",
            "schema": {}
          },
          "500": {
            "description": "Internal server error.",
            "schema": {}
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1RecordEventRequest"
            }
          }
        ],
        "tags": [
          "EventService"
        ]
      }
    },
    "/v1/session/{sessionId}": {
      "get": {
        "summary": "获取会话事件",
        "description": "获取指定会话的所有事件，按时间顺序返回。",
        "operationId": "EventService_GetAllSessionEvents",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1GetAllSessionEventsResponse"
            }
          },
          "404": {
            "description": "Resource not found.",
            "schema": {}
          },
          "500": {
            "description": "Internal server error.",
            "schema": {}
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "sessionId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "timestamp",
            "description": "事件发生时间",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "isDuringLiveSession",
            "description": "标识事件是否发生在已知的活跃会话期间",
            "in": "query",
            "required": false,
            "type": "boolean"
          }
        ],
        "tags": [
          "EventService"
        ]
      }
    }
  },
